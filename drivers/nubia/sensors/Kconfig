################################################# SENROS LOG
config FEATURE_NUBIA_SENSORS_LOG_ON
	tristate "NUBIA SENSORS LOG_ON"
	depends on NUBIA_SENSORS
	default n
	help
		Say Y here if you want to enable sensor log
################################################# PROX START
config NUBIA_SENSORS_PROX_PA224
	tristate "support litehole prox sensor driver"
	depends on NUBIA_SENSORS
	default n
	help
		nubia prox device of pa224
